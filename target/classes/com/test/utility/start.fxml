<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.test.controller.StartController"
          prefHeight="400.0" prefWidth="400.0"
          alignment="CENTER"
          hgap="10"
          vgap="10"
        >
    <padding>
        <Insets topRightBottomLeft="25"/>
    </padding>
    <Label GridPane.columnSpan="2" fx:id="WelcomeLabel" text="Welcome in the Aplication. If you are Student and want take a Quiz hit the Student button.If you are a teacher and you want to view student
results or create/modify tests hit Teacher button"
            GridPane.columnIndex="0" GridPane.rowIndex="0"
    />
    <Button   text="Teacher" fx:id="TeacherBtn" onAction="#onTeacher" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Button  text="Student" fx:id="StudentBtn" GridPane.columnIndex="1" GridPane.rowIndex="1"/>

</GridPane>
